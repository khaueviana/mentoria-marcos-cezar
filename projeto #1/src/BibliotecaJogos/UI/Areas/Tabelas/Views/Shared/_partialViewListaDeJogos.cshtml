@model IEnumerable<UI.Areas.Cadastros.ViewModels.JogoViewModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <table>
            <thead>
                <tr>
                    <th>Nome do Jogo</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr><td>@Html.DisplayFor(modelItem =>item.Nome)</td></tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
